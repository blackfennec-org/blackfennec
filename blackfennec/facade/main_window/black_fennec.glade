<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="BlackFennecView" parent="GtkApplicationWindow">
    <property name="width-request">400</property>
    <property name="height-request">300</property>
    <property name="visible">1</property>
    <property name="focusable">1</property>
    <property name="has-focus">1</property>
    <property name="window-position">center</property>
    <property name="default-width">1600</property>
    <property name="default-height">900</property>
    <property name="has-resize-grip">True</property>
    <child>
      <object class="GtkGrid" id="main-grid">
        <property name="visible">0</property>
        <child>
          <object class="GtkPaned">
            <property name="visible">0</property>
            <property name="focusable">1</property>
            <property name="hexpand">1</property>
            <property name="vexpand">1</property>
            <property name="orientation">vertical</property>
            <property name="wide-handle">1</property>
            <child>
              <object class="GtkGrid">
                <property name="visible">0</property>
                <property name="height-request">449</property>
                <child>
                  <object class="GtkPaned">
                    <property name="visible">0</property>
                    <property name="resize-start-child">0</property>
                    <property name="focusable">1</property>
                    <property name="hexpand">1</property>
                    <property name="vexpand">1</property>
                    <property name="position">250</property>
                    <property name="position-set">1</property>
                    <property name="wide-handle">1</property>
                    <child>
                      <object class="GtkScrolledWindow">
                        <property name="visible">0</property>
                        <property name="focusable">1</property>
                        <property name="child">
                          <object class="GtkTreeView" id="_file_tree">
                            <property name="visible">0</property>
                            <property name="width-request">100</property>
                            <property name="focusable">1</property>
                            <property name="vexpand">1</property>
                            <property name="enable-grid-lines">vertical</property>
                            <signal name="row-activated" handler="on_file_clicked" swapped="no"/>
                            <child internal-child="selection">
                              <object class="GtkTreeSelection"/>
                            </child>
                            <style>
                              <class name="main-file-tree"/>
                            </style>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkScrolledWindow">
                        <property name="visible">0</property>
                        <property name="focusable">1</property>
                        <property name="child">
                          <object class="GtkViewport">
                            <property name="visible">0</property>
                            <property name="shadow-type">none</property>
                            <property name="child">
                              <object class="GtkNotebook" id="_presenter_container">
                                <property name="visible">0</property>
                                <property name="width-request">200</property>
                                <property name="focusable">1</property>
                                <property name="scrollable">1</property>
                                <child>
                                  <object class="GtkNotebookPage">
                                    <property name="tab-expand">1</property>
                                    <property name="child">
                                      <object class="GtkBox" id="_empty_list_pattern">
                                        <property name="visible">0</property>
                                        <property name="name">empty-list-pattern</property>
                                        <property name="valign">center</property>
                                        <property name="margin-start">32</property>
                                        <property name="margin-end">32</property>
                                        <property name="margin-start">32</property>
                                        <property name="margin-end">32</property>
                                        <property name="hexpand">1</property>
                                        <property name="vexpand">1</property>
                                        <property name="orientation">vertical</property>
                                        <property name="spacing">8</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="visible">0</property>
                                            <property name="file">src/black_fennec/facade/main_window/icon_folder.png</property>
                                            <property name="pixel-size">128</property>
                                            <property name="icon_size">6</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="visible">0</property>
                                            <property name="label" translatable="1">Nothing to show...</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkButton">
                                            <property name="visible">0</property>
                                            <property name="label" translatable="1">Open Project</property>
                                            <property name="focusable">1</property>
                                            <property name="receives-default">1</property>
                                            <property name="halign">center</property>
                                            <property name="hexpand">0</property>
                                            <signal name="clicked" handler="on_open_clicked" swapped="no"/>
                                          </object>
                                        </child>
                                        <style>
                                          <class name="empty-list-pattern"/>
                                        </style>
                                      </object>
                                    </property>
                                    <property name="tab">
                                      <object class="GtkLabel">
                                        <property name="visible">0</property>
                                      </object>
                                    </property>
                                  </object>
                                </child>
                                <child>
                                  <placeholder/>
                                </child>
                                <child>
                                  <placeholder/>
                                </child>
                                <style>
                                  <class name="presenter-container"/>
                                  <class name="hide-tab"/>
                                </style>
                              </object>
                            </property>
                          </object>
                        </property>
                      </object>
                    </child>
                    <layout>
                      <property name="column">0</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkTextView">
                <property name="visible">0</property>
                <property name="focusable">1</property>
                <property name="hexpand">0</property>
                <property name="left-margin">15</property>
                <property name="right-margin">15</property>
                <property name="top-margin">15</property>
                <property name="bottom-margin">15</property>
                <property name="input-purpose">terminal</property>
                <style>
                  <class name="main-status-bar"/>
                </style>
              </object>
            </child>
            <layout>
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <style>
          <class name="main-main-grid"/>
        </style>
      </object>
    </child>
    <child type="titlebar">
      <object class="GtkBox">
        <property name="visible">0</property>
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkHeaderBar" id="header_bar">
            <property name="visible">0</property>
            <property name="hexpand">1</property>
            <child type="title">
              <object class="GtkLabel">
                <property name="visible">0</property>
                <property name="label" translatable="1">Black Fennec</property>
              </object>
            </child>
            <child>
              <object class="GtkImage">
                <property name="visible">0</property>
                <property name="file">src/black_fennec/facade/main_window/window_icon.png</property>
                <style>
                  <class name="window-icon"/>
                </style>
              </object>
            </child>
            <child>
              <object class="GtkMenuBar">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <child>
                  <object class="GtkMenuItem" id="_open">
                    <property name="visible">True</property>
                    <property name="can-focus">False</property>
                    <property name="label" translatable="1">_Project</property>
                    <property name="use-underline">True</property>
                    <child type="submenu">
                      <object class="GtkMenu">
                        <property name="visible">True</property>
                        <property name="can-focus">False</property>
                        <child>
                          <object class="GtkImageMenuItem">
                            <property name="label">gtk-new</property>
                            <property name="visible">True</property>
                            <property name="can-focus">False</property>
                            <property name="use-underline">True</property>
                            <property name="use-stock">True</property>
                            <signal name="activate" handler="on_new_clicked" swapped="no"/>
                            <style>
                              <class name="menu-item"/>
                            </style>
                          </object>
                        </child>
                        <child>
                          <object class="GtkImageMenuItem">
                            <property name="label">gtk-open</property>
                            <property name="visible">True</property>
                            <property name="can-focus">False</property>
                            <property name="use-underline">True</property>
                            <property name="use-stock">True</property>
                            <signal name="activate" handler="on_open_clicked" swapped="no"/>
                            <style>
                              <class name="menu-item"/>
                            </style>
                          </object>
                        </child>
                        <child>
                          <object class="GtkImageMenuItem">
                            <property name="label">gtk-quit</property>
                            <property name="visible">True</property>
                            <property name="can-focus">False</property>
                            <property name="use-underline">True</property>
                            <property name="use-stock">True</property>
                            <signal name="activate" handler="on_quit_clicked" swapped="no"/>
                            <style>
                              <class name="close"/>
                              <class name="last-item"/>
                              <class name="menu-item"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                    <style>
                      <class name="main-menu-item"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkMenuItem" id="_save">
                    <property name="visible">True</property>
                    <property name="can-focus">False</property>
                    <property name="label" translatable="1">_Save</property>
                    <property name="use-underline">True</property>
                    <child type="submenu">
                      <object class="GtkMenu">
                        <property name="visible">True</property>
                        <property name="can-focus">False</property>
                        <child>
                          <object class="GtkImageMenuItem">
                            <property name="label">gtk-save</property>
                            <property name="visible">True</property>
                            <property name="can-focus">False</property>
                            <property name="use-underline">True</property>
                            <property name="use-stock">True</property>
                            <signal name="activate" handler="on_save_clicked" swapped="no"/>
                            <style>
                              <class name="menu-item"/>
                            </style>
                          </object>
                        </child>
                        <child>
                          <object class="GtkImageMenuItem">
                            <property name="label">gtk-save-as</property>
                            <property name="visible">True</property>
                            <property name="can-focus">False</property>
                            <property name="use-underline">True</property>
                            <property name="use-stock">True</property>
                            <signal name="activate" handler="on_save_as_clicked" swapped="no"/>
                            <style>
                              <class name="menu-item"/>
                              <class name="last-item"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                    <style>
                      <class name="main-menu-item"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkMenuItem" id="_go_to_store">
                    <property name="visible">True</property>
                    <property name="can-focus">False</property>
                    <property name="label" translatable="1">_Go to Store</property>
                    <property name="use-underline">True</property>
                    <signal name="activate" handler="on_go_to_store_clicked" swapped="no"/>
                    <style>
                      <class name="main-menu-item"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkMenuItem" id="_about_and_help">
                    <property name="visible">True</property>
                    <property name="can-focus">False</property>
                    <property name="label" translatable="1">_About &amp; Help</property>
                    <property name="use-underline">True</property>
                    <signal name="activate" handler="on_about_and_help_clicked" swapped="no"/>
                    <style>
                      <class name="main-menu-item"/>
                    </style>
                  </object>
                </child>
                <style>
                  <class name="main-menu-bar"/>
                </style>
              </object>
            </child>
            <child>
              <object class="GtkSearchEntry">
                <property name="visible">0</property>
                <property name="focusable">1</property>
                <property name="max-length">10</property>
                <property name="shadow-type">none</property>
                <property name="primary-icon-name">edit-find-symbolic</property>
                <property name="primary-icon-activatable">False</property>
                <property name="primary-icon-sensitive">False</property>
                <property name="placeholder-text" translatable="1">Search</property>
                <style>
                  <class name="search-entry"/>
                </style>
              </object>
            </child>
            <style>
              <class name="main-header-bar"/>
            </style>
          </object>
        </child>
      </object>
    </child>
    <style>
      <class name="main-window"/>
    </style>
  </template>
</interface>
