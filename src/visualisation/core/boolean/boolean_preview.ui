<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="BooleanPreview" parent="AdwBin">
    <property name="visible">true</property>
    <property name="focusable">false</property>
    <child>
      <object class="GtkSwitch" id="_value">
        <property name="focusable">true</property>
        <signal name="state-set" handler="_on_state_changed"/>
        <style>
          <class name="boolean"/>
        </style>
      </object>
    </child>
  </template>
</interface>