        </child>
        <child>
          <object class="GtkLabel">
            <layout>
              <property name="transform">translate(248, 323)</property>
            </layout>
            <property name="width-request">100</property>
            <property name="height-request">80</property>
            <property name="label" translatable="true">Caspar Martens, Simon Kindhauser,
Leonie DÃ¤ullary, Lara Gubler</property>
